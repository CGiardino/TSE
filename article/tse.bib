	Keywords = {Animals,Cochlea,Cochlea: physiology,Computer Simulation,Humans,Linear Models,Microcomputers,Models, Biological,Nonlinear Dynamics,Otoacoustic Emissions, Spontaneous,Otoacoustic Emissions, Spontaneous: physiology,Reaction Time,Reaction Time: physiology},
	Month = nov,
