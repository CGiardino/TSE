	Journal = {Harvard Business Review},
	Keywords = {commerce,commerce organization \& administration,commerce standards,consumer satisfaction,consumer satisfaction economics,financial audit,financial audit methods,management audit,management audit methods,management information systems,management information systems standards,organizational innovation,organizational objectives,planning techniques,united states},
	Number = {1},
