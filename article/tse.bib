	Publisher = {IEE},
	Series = {"Third International Workshop on Global Software Development (GSD 2004)" W12S Workshop - 26th International Conference on Software Engineering},
	Title = {{Transitioning from a co-located to a globally-distributed software development team: a case study at Analog Devices Inc}},
	Volume = {2004},
	Year = {2004},
	Bdsk-Url-1 = {http://dx.doi.org/10.1049/ic:20040303}}

